	"description": "",
	"dependencies": {
		"drizzle-orm": "^0.39.0",
		"pg": "^8.13.1",
		"postgres": "^3.4.5"
	},
	"devDependencies": {
		"@tantovale/eslint-config": "*",
		"@tantovale/typescript-config": "*",
		"drizzle-kit": "^0.30.3"
	}
}
